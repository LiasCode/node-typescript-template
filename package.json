{
  "name": "template-node-typescript",
  "version": "1.0.0",
  "description": "Template for use Typescript in nodejs",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/main.js",
    "build": "tsc -p ./tsconfig.json",
    "dev": "ts-node-dev --watch ./src  --clear --ignore-watch node_modules ./src/main.ts",
    "format": "prettier --write 'src/**/*.{js,ts,json}' --config .prettierrc.json",
    "check-types": "tsc --noEmit -p ./tsconfig.json"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "template"
  ],
  "author": "LiasCode",
  "dependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/jsonwebtoken": "9.0.5",
    "@types/uuid": "9.0.7",
    "axios": "1.6.2",
    "bcrypt": "5.1.1",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "helmet": "7.1.0",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "pg": "8.11.3",
    "redis": "4.6.12",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "20.10.5",
    "@types/pg": "8.10.9",
    "concurrently": "8.2.2",
    "prettier": "3.1.1",
    "ts-node": "10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.3.3"
  }
}
