{
  "name": "template-node-typescript",
  "version": "1.0.0",
  "description": "Template for us nodejs with typescript",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node ./built/main.js",
    "build": "tsc",
    "dev": "nodemon --config ./nodemon.json",
    "lint": "prettier --write 'src/**/*.{js,ts,json}' --config .prettierrc.json"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "template"
  ],
  "author": "LiasCode",
  "dependencies": {
    "axios": "^1.2.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.15.3",
    "nodemon": "^2.0.20",
    "prettier": "2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
