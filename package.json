{
  "name": "template-node-typescript-express",
  "version": "1.0.0",
  "description": "Starter template for use typescript and express in node",
  "main": "./dist/main.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/main.js",
    "build": "tsc -p ./tsconfig.json",
    "dev": "tsx --watch ./src/main.ts",
    "format": "prettier ./src --ignore-unknown --write --config .prettierrc.json"
  },
  "keywords": [
    "node",
    "typescript",
    "template",
    "express",
    "nodejs",
    "sqlite"
  ],
  "author": "LiasCode",
  "dependencies": {
    "bcrypt": "5.1.1",
    "better-sqlite3": "11.5.0",
    "body-parser": "1.20.3",
    "compression": "1.7.4",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "date-fns": "4.1.0",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "express-promise-router": "4.1.1",
    "helmet": "7.1.0",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/better-sqlite3": "7.6.11",
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jsonwebtoken": "9.0.6",
    "@types/morgan": "1.9.9",
    "@types/node": "20.12.7",
    "@types/pg": "8.11.5",
    "@types/uuid": "9.0.8",
    "prettier": "3.2.5",
    "tsx": "^4.19.2",
    "typescript": "5.4.5"
  }
}
