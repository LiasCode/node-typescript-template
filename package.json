{
  "name": "template-node-typescript",
  "version": "1.0.0",
  "description": "Template for us nodejs with typescript",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node ./built/main.js",
    "build": "tsc -p ./tsconfig.json",
    "dev": "nodemon --config ./nodemon.json",
    "format": "prettier --write 'src/**/*.{js,ts,json}' --config .prettierrc.json"
  },
  "keywords": [
    "node",
    "nodejs",
    "typescript",
    "template"
  ],
  "author": "LiasCode",
  "dependencies": {
    "axios": "1.6.2",
    "body-parser": "1.20.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.1.0",
    "http-status-codes": "2.3.0",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.5",
    "@types/compression": "1.7.5",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "20.10.5",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
